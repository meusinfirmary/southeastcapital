{
    "name": "sirna/sec",
    "description": "project package for southeast capital website",
    "license": "proprietary",
    "homepage": "http://sec.dev",
    "authors": [
        {
            "name": "Zabka Sirna Gwen",
            "email": "zabka.sirna.gwen@gmail.com",
            "homepage": "http://sirna.id",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.2.3",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.2.3.zip"
                },
                "require": {
                    "fancyguy/webroot-installer": "1.1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "wordpress": "4.2.3",
        "fancyguy/webroot-installer": "1.1.0"
    },
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 1000
    },
    "extra": {
        "webroot-dir": "site",
        "webroot-package": "wordpress",
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "require-dev": {
        "dcarbone/dom-plus": "1.3.1"
    }
}
